  "author": "Abolaji <abolajiadeajayi@gamil.com>",
  "license": "MIT",
  "keywords": [
    "api",
    "express",
    "typescript",
    "mysql",
    "sequelize",
    "application-monitoring",
    "logging",
    "task-management",
    "analytics",
    "vercel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abolajii/apphubBEv1.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc --watch",
